source 'https://github.com/CocoaPods/Specs.git'

xcodeproj ‘CloneTinder.xcodeproj'
target ‘CloneTinder’ do
    pod 'Firebase/Core'
    pod 'Firebase/Database'
    pod 'Firebase/Auth'
    pod 'GoogleSignIn'
    pod ‘FBSDKCoreKit’
    pod ‘FBSDKShareKit’
    pod ‘FBSDKLoginKit’
    workspace ‘CloneTinder.xcworkspace'

use_frameworks!
end